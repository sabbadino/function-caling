{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Default": "Debug",
        "Microsoft.AspNetCore": "Debug",
        "Grpc": "Debug"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ]
  }
}
